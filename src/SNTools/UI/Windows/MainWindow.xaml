<UserControl x:Class="SNTools.UI.Windows.MainWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:SNTools.UI.Windows"
             xmlns:custom="clr-namespace:SNTools.UI.Controls"
             mc:Ignorable="d" 
             Width="1000" Height="700">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/UI/MainStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <custom:SubWindow Title="SN Tools">
        <custom:SubWindow.WindowContent>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                <Button Width="200" Height="40" Margin="10">Unlock Skins</Button>
                <CheckBox Width="50" Height="20" Margin="10">Hi</CheckBox>
            </StackPanel>
        </custom:SubWindow.WindowContent>
    </custom:SubWindow>
</UserControl>
